{
  "name": "mh-ui",
  "version": "0.31.5",
  "description": "Improve your MouseHunt experience.",
  "author": "Brad Parbs <brad@bradparbs.com> (https://bradparbs.com/)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "concurrently \"bun:build:extension\" \"bun:build:archive\" \"bun:build:userscript\" \"bun:build:userscript:all\"",
    "build:archive": "mkdir -p dist && git archive --format zip --output dist/archive.zip HEAD",
    "build:extension": "concurrently \"bun:build:extension:*\"",
    "build:extension:chrome": "node scripts/build-extension.mjs chrome",
    "build:extension:firefox": "node scripts/build-extension.mjs firefox",
    "build:userscript": "node scripts/build-userscript.mjs",
    "build:userscript:all": "node scripts/build-individual-userscripts.mjs",
    "clean": "del-cli dist",
    "lint:css:fix": "stylelint **/**/*.css --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:fix": "concurrently \"bun:lint:*:fix\"",
    "lint:js:fix": "eslint --ext .js src --fix",
    "lint:js": "eslint --ext .js src",
    "lint:json:fix": "eslint --ext .json . --fix",
    "lint:json": "eslint --ext .json .",
    "lint:md:fix": "eslint --ext .md . --fix",
    "lint:md": "eslint --ext .md .",
    "lint": "concurrently \"bun:lint:*(!fix)\"",
    "lint:scripts": "eslint --ext .mjs scripts",
    "lint:scripts:fix": "eslint --ext .mjs scripts --fix",
    "fix-map-groups": "node scripts/fix-map-groups.mjs"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@sentry/browser": "^7.102.0",
    "humanize-duration": "^3.31.0"
  },
  "devDependencies": {
    "@sentry/cli": "^2.28.6",
    "@sprout2000/esbuild-copy-plugin": "^1.1.13",
    "@wordpress/eslint-plugin": "^17.8.0",
    "archiver": "^6.0.1",
    "concurrently": "^8.2.2",
    "del-cli": "^5.1.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-newlines": "^1.3.4",
    "eslint-plugin-jsdoc": "^48.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdownlint": "^0.5.0",
    "eslint-plugin-package-json": "^0.10.4",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-jsdoc": "^1.0.4",
    "eslint-plugin-unicorn": "^51.0.1",
    "fast-glob": "^3.3.2",
    "json-minify": "^1.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "prettier": "^3.2.5",
    "pretty-ms": "^9.0.0",
    "stylelint": "15.11.0",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-recommended": "14.0.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.0",
    "stylelint-stylistic": "^0.4.5",
    "yargs": "^17.7.2"
  }
}
