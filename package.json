{
  "name": "mh-ui",
  "version": "0.24.1",
  "description": "Improve your MouseHunt experience.",
  "author": "Brad Parbs <brad@bradparbs.com> (https://bradparbs.com/)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "concurrently \"bun:build:*\"",
    "build:archive": "mkdir dist && git archive --format zip --output dist/archive.zip HEAD",
    "build:extension": "concurrently \"bun:build:extension:*\"",
    "build:extension:chrome": "node scripts/build-extension.mjs chrome",
    "build:extension:firefox": "node scripts/build-extension.mjs firefox",
    "build:userscript": "node scripts/build-userscript.mjs",
    "clean": "del-cli dist",
    "lint:css:fix": "stylelint **/*.css --fix",
    "lint:css": "stylelint **/*.css",
    "lint:fix": "concurrently \"bun:lint:*:fix\"",
    "lint:js:fix": "eslint --ext .js . --fix -c .eslintrc.json",
    "lint:js": "eslint --ext .js . -c .eslintrc.json",
    "lint:md:fix": "eslint --ext .md . --fix -c .eslintrc.json",
    "lint:md": "eslint --ext .md . -c .eslintrc.json",
    "lint": "concurrently \"bun:lint:*(!fix)\"",
    "prebuild": "del-cli dist"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.4",
    "humanize-duration": "^3.31.0",
    "mousehunt-utils": "^1.11.0",
    "pretty-ms": "^8.0.0"
  },
  "devDependencies": {
    "@wordpress/eslint-plugin": "^17.2.0",
    "archiver": "^6.0.1",
    "concurrently": "^8.2.2",
    "del-cli": "^5.1.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-markdownlint": "^0.5.0",
    "eslint-plugin-prettier": "^5.0.1",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3"
  }
}
