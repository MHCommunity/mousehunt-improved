{
  "name": "mh-ui",
  "version": "0.43.1",
  "description": "Improve your MouseHunt experience.",
  "author": "Brad Parbs <brad@bradparbs.com> (https://bradparbs.com/)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build:archive": "mkdir -p dist && git archive --format zip --output dist/archive.zip HEAD",
    "build:extension:chrome": "node scripts/build-extension.mjs chrome",
    "build:extension:firefox": "node scripts/build-extension.mjs firefox",
    "build:extension": "concurrently \"bun:build:extension:*\"",
    "build:userscript:all": "node scripts/build-userscript-all.mjs",
    "build:userscript": "node scripts/build-userscript.mjs",
    "build:zips": "node scripts/build-zips.mjs",
    "build": "concurrently \"bun:build:extension:chrome\" \"bun:build:extension:firefox\" \"bun:build:archive\" \"bun:build:userscript\" \"bun:build:zips\"",
    "clean": "del-cli dist",
    "dev:chrome": "node scripts/build-extension.mjs chrome watch",
    "dev:firefox": "node scripts/build-extension.mjs firefox watch",
    "dev": "node scripts/build-extension.mjs chrome watch",
    "fix-map-groups": "node scripts/fix-map-groups.mjs",
    "lint:css:fix": "stylelint **/**/*.css --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:fix": "concurrently \"bun:lint:*:fix\"",
    "lint:js:fix": "eslint src/**/*.js --fix",
    "lint:js": "eslint src/**/*.js",
    "lint:json:fix": "eslint **/*.json --fix",
    "lint:json": "eslint **/*.json .",
    "lint:md:fix": "eslint **/*.md --fix",
    "lint:md": "eslint **/*.md",
    "lint:scripts:fix": "eslint scripts/**/*.mjs --fix",
    "lint:scripts": "eslint scripts/**/*.mjs",
    "lint": "concurrently \"bun:lint:*(!fix)\"",
    "release": "./scripts/release"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.1",
    "@sentry/browser": "^7.109.0",
    "humanize-duration": "^3.32.0"
  },
  "devDependencies": {
    "@sentry/cli": "^2.31.0",
    "@sprout2000/esbuild-copy-plugin": "^1.1.13",
    "@wordpress/eslint-plugin": "^17.11.0",
    "archiver": "^7.0.1",
    "concurrently": "^8.2.2",
    "del-cli": "^5.1.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-jsdoc": "^48.2.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdownlint": "^0.5.0",
    "eslint-plugin-package-json": "^0.12.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-jsdoc": "^1.0.4",
    "eslint-plugin-unicorn": "^51.0.1",
    "fast-glob": "^3.3.2",
    "json-minify": "^1.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "prettier": "^3.2.5",
    "pretty-ms": "^9.0.0",
    "sentry-cli": "^0.0.0",
    "stylelint": "15.11.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended": "13.0.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "4.1.0",
    "stylelint-stylistic": "^0.4.3",
    "yargs": "^17.7.2"
  }
}
